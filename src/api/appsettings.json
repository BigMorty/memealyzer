{
  "AzureDefaults":{
    "Retry": {
      "MaxRetries": 10,
      "Delay": "0:0:3"
    },
    "Diagnostics":
    {
      "IsLoggingEnabled": true
    }
  },
  "Logging": {
    "LogLevel": {
      "Azure": "Information"
    },
    "ApplicationInsights": {
      "LogLevel": {
        "SimpleTracingPolicy": "Information",
        "Azure": "Information"
      }
    }
  },
  "AllowedHosts": "*"
}
