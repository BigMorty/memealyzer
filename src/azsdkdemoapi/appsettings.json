{
  "AzureDefaults":{
    "Retry": {
      "MaxRetries": 10,
      "Delay": "0:0:3"
    },
    "Diagnostics":
    {
      "IsLoggingEnabled": true
    }
  },
  "AzureBlobStorage": {
    "ServiceUri": ""
  },
  "ApplicationInsights": {
    "InstrumentationKey": ""
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Azure-Identity": "Information"
    },
    "ApplicationInsights": {
      "LogLevel": {
        "Azure-Identity": "Information"
      }
    }
  },
  "AllowedHosts": "*"
}
